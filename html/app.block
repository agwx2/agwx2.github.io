        css
            top 0
            left 0
            opacity 0
            width 100%
            z-index 50
            height 100%
            display none
            position fixed
            transition opacity 0.2s ease
            background-color rgba(250, 250, 250, 0.9)
        :show
            {
                setTimeout(function () {
                    block.css('display', 'block');
                    setTimeout(function () {
                        block.css('opacity', '1');
                    }, 10);
                }, 10);
            }
        :hide
            {
                setTimeout(function () {
                    block.css('opacity', '0');
                    setTimeout(function () {
                        block.css('display', 'none');
                    }, 210);
                }, 10);
            }
        :click
            {
                block.on('hide');
            }
        block imgwrap
            css
                opacity 0.9
            img img
                width 400px
                height 400px
                src img/snapcodes/snapcodeBitmoji.svg
                @query window width >= 550
                    css
                        width 400px
                        height 400px
                @query window width < 550
                    {
                        var width = window.innerWidth * 0.727272;
                        block.css({
                            width: width + 'px',
                            height: width + 'px'
                        });
                    }
                :click
                    {
                        event.stopPropagation();
                    }
        div x
            css
                position absolute
                top 80px
                left 25px
                opacity 0.65
                cursor pointer
                transition opacity 0.2s ease
            :mouseover
                css
                    opacity 0.75
            :mouseout
                css
                    opacity 0.65
            :click
                {
                    block.parent().on('hide');
                }
            image img
                src img/x.png
                height 35px
                width 35px

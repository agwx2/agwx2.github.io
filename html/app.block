                                        var y = topblock.$().offset().top - b.parent(1).$().offset().top + 100;
                                        var ripple = b.parent(1).child('ripple');
                                        var w = topblock.$().width() - 200;
                                        var h = topblock.$().height() - 200;
                                        var opacity = 40;
                                        var speed = 50;
                                        var grow;
                                        grow = function () {
                                            speed -= 0.005;
                                            if (speed <= 5)
                                                speed = 5;
                                            w += speed;
                                            h += speed;
                                            x -= speed / 2;
                                            y -= speed / 2;
                                            ripple.css({
                                                left: x + 'px',
                                                top: y + 'px',
                                                height: h + 'px',
                                                width: w + 'px'
                                            });

                bottom 0
                height 50%
                z-index -1
                background-color rgba(5, 5, 5, 0)
                transition background-color 0.4s ease, height 0.5s ease
            :hide
                {
                    block.css('height', '50%').css('background-color', 'rgba(5, 5, 5, 0)').parent().css('height', '60px').css('top', '-80px');
                }
            :show
                {
                    block.css('background-color', 'rgba(70, 70, 70, 0.975)').css('height', '100%').parent().css('height', '60px').css('top', '0');
                }
            {
                $(window).scroll(function () {
                    if ($(this).scrollTop() > block.parent().sibling('about').$().offset().top - 100)
                        block.on('show');
                    else block.on('hide');
                });
            }
        !menu-item
            init
                {
                    return Block('block')
                        .class('menu-item')
                        .css({
                            minWidth: '70px',
                            width: 'auto',
                            height: '100%',
                            display: 'inline-table',
                            cursor: 'pointer',
                            opacity: '0.9',
                            position: 'relative',
                            zIndex: '100'
                        })
                        .add(Block('block', 'content')
                            .css({
                                backgroundColor: 'rgba(0, 0, 0, 0)',
                                transition: 'opacity 0.2s ease, background-color 0.2s ease',
                                borderRadius: '5px',
                                textShadow: '0px 0px 40px rgba(50, 50, 50, 1)',
                                height: '65%',
                                padding: '0 15px',
                                width: 'auto'
                            })
                            .add(Block('text', 1)
                                .data('')
                                .css({
                                    textTransform: 'uppercase',
                                    fontSize: '17px',
                                    cursor: 'pointer',
                                    color: 'white',
                                    fontFamily: 'Source Sans Pro',
                                    fontWeight: '400',
                                    // letterSpacing: '1px'
                                })
                            )
                            .on('mouseover', function (e, b) {
                                b.css('opacity', '1').css('background-color', 'rgba(200, 200, 200, 0.15)');
                            })
                            .on('mouseout', function (e, b) {
                                b.css('opacity', '0.9').css('background-color', 'rgba(200, 200, 200, 0)');
                            })
                        )
                    ;
                }
            load
                {
                    data('val', function (val) {
                        block.child('content/text').html('').data(val);
                    });
                    data('scrolltop', function (st) {
                        block.on('click', function () {
                            $('html, body').stop().animate({ scrollTop: st }, 1200, 'swing');
                        });
                    });
                    data('page', function (page) {
                        block.on('click', function () {
                            $('html, body').stop().animate({ scrollTop: window.app.block.child(page).$().offset().top - 100 }, 1200, 'swing');
                        });
                    });
                    data('pageID', function (pageID) {
                        block.on('click', function () {
                            $('html, body').stop().animate({ scrollTop: $('#' + pageID).offset().top - 100 }, 1200, 'swing');
                        });
                    });
                }

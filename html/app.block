                                    .on('click', function (e, b) {
                                        topblock.sibling('ripple').css('display', 'block');
                                        // topblock.sibling('pages').on('top');
                                        var fullwidth = 3 * (window.innerHeight > window.innerWidth ? window.innerHeight : window.innerWidth);
                                        var x = topblock.$().offset().left - b.parent(1).$().offset().left + 100;
                                        var y = topblock.$().offset().top - b.parent(1).$().offset().top + 100;
                                        var ripple = b.parent(1).child('ripple');
                                        var w = topblock.$().width() - 200;
                                        var h = topblock.$().height() - 200;
                                        var opacity = 40;
                                        var speed = 90;
                                        var grow;
                                        grow = function () {
                                            speed -= 0.005;
                                            if (speed <= 5)
                                                speed = 5;
                                            w += speed;
                                            h += speed;
                                            x -= speed / 2;
                                            y -= speed / 2;
                                            ripple.css({
                                                left: x + 'px',
                                                top: y + 'px',
                                                height: h + 'px',
                                                width: w + 'px'
                                            });
                                            opacity += 3;
                                            if (opacity > 100)
                                                opacity = 100;
                                            ripple.css('background-color', 'rgba(255, 255, 255, ' + (opacity / 100) + ')');
                                            if (w < fullwidth || h < fullwidth || opacity < 1)
                                                setTimeout(grow, 10);
                                            else b.parent(1).child('pages').on('show', { title: b.parent().mark() });
                                        };
                                        grow();
                                    })
                                )
                            ;
                            return topblock;
                        }
